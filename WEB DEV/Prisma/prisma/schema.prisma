generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model User{
  id        Int @default(autoincrement()) @id
  usename   String @unique
  password  String
  age       Int
  todos     Todo[]
}

model Todo{
  id          Int @default(autoincrement()) @id
  title       String
  description String
  done        Boolean
  userId      Int
  user        User @relation(fields: [userId] , references: [id])
}